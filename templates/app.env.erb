NODE_ENV=<%= @deploy[:nodejs][:env] %>
PORT=<%= @deploy[:nodejs][:port] %>
APP_PORT=<%= @deploy[:nodejs][:port] %>

DB_HOST=<%= @db_host %>
DB_PORT=<%= @db_port %>
DB_DATABASE=<%= @db_database %>
DB_USER=<%= @db_username %>
DB_USERNAME=<%= @db_username %>
DB_PASSWORD=<%= @db_password %>
DB_DIALECT=<%= @deploy[:nodejs][:dialect] %>

<% if @deploy[:apps] -%>
<% if @deploy[:apps][:isoft] -%>
FE_CLIENT_ID=<%= @deploy[:apps][:isoft][:fe_client_id] %>
FE_REDIRECT_URI=<%= @deploy[:apps][:isoft][:fe_redirect_uri] || "http://#{@deploy[:domains].first}/ecomcallback" %>
FE_DASHBOARD_URI=<%= @deploy[:apps][:isoft][:fe_dashboard_uri] || "http://#{@deploy[:domains].first}/" %>
ISOFT_API_HOST=<%= @deploy[:apps][:isoft][:host] || "api.infusionsoft.com" %>
ISOFT_API_PORT=<%= @deploy[:apps][:isoft][:port] || "443" %>
ISOFT_API_PATH=<%= @deploy[:apps][:isoft][:path] || "/crm/xmlrpc/v1" %>
ISOFT_PERMISSION_URL=<%= @deploy[:apps][:isoft][:permission_url] %>
ISOFT_TOKEN_URL=<%= @deploy[:apps][:isoft][:token_url] || "https://api.infusionsoft.com/token" %>
ISOFT_CLIENT_ID=<%= @deploy[:apps][:isoft][:client_id] %>
ISOFT_CLIENT_SECRET=<%= @deploy[:apps][:isoft][:client_id_secret] %>
ISOFT_REDIRECT_URL=<%= @deploy[:apps][:isoft][:redirect_url] %>
<% end -%>
APP_EXPIRE=<%= @deploy[:apps][:expire] %>
<% if @deploy[:apps][:amazon] -%>
APP_AMAZON_AWSID=<%= @deploy[:apps][:amazon][:awsid] %>
APP_AMAZON_SECRET=<%= @deploy[:apps][:amazon][:secret] %>
APP_AMAZON_AWSTAGS=<%= @deploy[:apps][:amazon][:tags] %>
<% end -%>

<% if @deploy[:apps][:adwords] -%>
APP_ADWORDS_USERNAME=<%= @deploy[:apps][:adwords][:username] %>
APP_ADWORDS_PASSWORD=<%= @deploy[:apps][:adwords][:password] %>
<% end -%>

<% if @deploy[:apps][:aliexpress] -%>
APP_ALIEXPRESS_TOKEN=<%= @deploy[:apps][:aliexpress][:token] %>
<% end -%>
<% if @deploy[:apps][:youtube] -%>
APP_YOUTUBE_TOKEN=<%= @deploy[:apps][:youtube][:token] %>
<% end -%>
<% if @deploy[:apps][:jwt_secret] -%>
JWT_SECRET=<%= @deploy[:apps][:jwt_secret] %>
<% end -%>
<% end -%>
